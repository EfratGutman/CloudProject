	<script >
            function fun1()
      {
        var id = document.getElementById('fld_base_profile_id').value;
        alert(id);
        getNut(id);
      }

      function getJson(str) {
      var xmlhttp = new XMLHttpRequest();
      xmlhttp.onreadystatechange = function () {
      if (this.readyState == 4 && this.status == 200) {
      //הפיכת הטקסט למערך בזכרון
      var result = JSON.parse(this.responseText);
      var foods = result["list"]["item"];
      var el = document.getElementById('fld_base_profile_id');

      for (var i = 0; i < foods.length; i++) {
                                    var dish = foods[i],
                                        opt = document.createElement("option");
                                    
                                    opt.value = dish.ndbno;
                                    opt.text = dish.name;
                                    opt.id = dish.id;
                                  
                                    el.appendChild(opt);
                                }
                            }
                        };
                        xmlhttp.open("GET", "https://api.nal.usda.gov/ndb/search/?format=json&q="+str+"&sort=n&max=25&offset=0&api_key=DEMO_KEY", true);
      xmlhttp.send();
      }






      function getNut(id) {
      var xmlhttp = new XMLHttpRequest();
      xmlhttp.onreadystatechange = function () {
      if (this.readyState == 4 && this.status == 200) {
      //הפיכת הטקסט למערך בזכרון
      var result = JSON.parse(this.responseText);
      var nuts = result["report"]["foods"];
      var name = nuts[0].name;
      var cal = nuts[0].nutrients[0].value;
      var sugar = nuts[0].nutrients[1].value;
      var lipid = nuts[0].nutrients[2].value;
      var carbohydrate = nuts[0].nutrients[3].value;



	  

      document.getElementById('b1').value = cal;
      document.getElementById('b2').value = carbohydrate;
      document.getElementById('b3').value = lipid;
	   document.getElementById('b4').value = sugar;
      window.x = { name, cal, sugar, lipid, carbohydrate };
	  

      }
      };
      xmlhttp.open("GET", "https://api.nal.usda.gov/ndb/nutrients/?format=json&api_key=DEMO_KEY&nutrients=205&nutrients=204&nutrients=208&nutrients=269&ndbno=" + id, true);
      xmlhttp.send();
      }





</script>




